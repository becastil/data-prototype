{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "dev": "next dev -p 3005",
    "build": "next build",
    "install:legacy": "npm install --legacy-peer-deps",
    "build:amplify": "npm run install:legacy && npm run build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:analyze:win": "powershell -Command \"$env:ANALYZE='true'; npm run build\"",
    "start": "NODE_ENV=production next start -p $PORT",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "win:install": "powershell -ExecutionPolicy Bypass -File ./scripts/install.ps1",
    "win:dev": "powershell -ExecutionPolicy Bypass -File ./scripts/dev.ps1",
    "win:build": "powershell -ExecutionPolicy Bypass -File ./scripts/build.ps1",
    "win:build:analyze": "powershell -ExecutionPolicy Bypass -File ./scripts/build.ps1 -Analyze",
    "win:troubleshoot": "powershell -ExecutionPolicy Bypass -File ./scripts/troubleshoot.ps1",
    "win:cleanup": "powershell -ExecutionPolicy Bypass -File ./scripts/cleanup.ps1",
    "win:help": "powershell -Command \"Get-Help ./scripts/install.ps1; Get-Help ./scripts/dev.ps1; Get-Help ./scripts/build.ps1\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@formkit/auto-animate": "^0.8.4",
    "@motionone/dom": "^10.18.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@rive-app/canvas": "^2.31.2",
    "@rive-app/react-canvas": "^4.23.1",
    "@theatre/core": "^0.7.2",
    "@theatre/studio": "^0.7.2",
    "@types/react-window": "^1.8.8",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/next-plugin": "^2.4.14",
    "@vanilla-extract/recipes": "^0.5.7",
    "@vanilla-extract/sprinkles": "^1.6.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "framer-motion": "^12.23.12",
    "fuse.js": "^7.1.0",
    "gsap": "^3.13.0",
    "immer": "^10.1.1",
    "jspdf": "^3.0.2",
    "lottie-react": "^2.4.1",
    "lottie-web": "^5.13.0",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-window": "^1.8.10",
    "react-window-infinite-loader": "^1.0.10",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "critters": "^0.0.22",
    "cross-env": "^7.0.3",
    "esbuild-loader": "^4.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "xo": "^0.59.0"
  },
  "xo": {
    "extensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "prettier": true,
    "ignores": [
      ".next/**",
      "node_modules/**",
      "docs/**"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "xo --fix",
      "prettier --write"
    ],
    "*.{json,yaml,yml,md}": [
      "prettier --write"
    ],
    "*.py": [
      "ruff format",
      "ruff check --fix"
    ]
  }
}
